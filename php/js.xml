<scripts>
	<row>
	<id>add</id>
	<params>
		<param>firstParam</param>
		<param>secondParam</param>
	</params>
	<script>retVal = firstParam + secondParam;</script>
	</row>
	<row>
	<id>multi</id>
	<params>
		<param>firstParam</param>
		<param>secondParam</param>
	</params>
	<script>retVal = firstParam * secondParam;</script>
	</row>
	<row>
	<id>div</id>
	<params>
		<param>firstParam</param>
		<param>secondParam</param>
	</params>
	<script>retVal = firstParam / secondParam;</script>
	</row>
	<row>
	<id>sumArr</id>
	<params>
		<param>myArray</param>
	</params>
	<script>
	var sum = 0;
	myArray = [4, 7];
	var len = myArray.length;
	for(i = 0; i &lt; len; i++)
	{
		sum += myArray[i];
	}
	retVal = sum;
	</script>
	</row>
</scripts>
